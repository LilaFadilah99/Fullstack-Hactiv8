<%- include("partials/header") %>
<div class="container my-5">
  <div class="container-fluid">
    <h1>Author List</h1>
    <br /><br />
    <!-- Table -->
    <table class="table">
      <thead>
        <tr>
          <th scope="col">No.</th>
          <th scope="col">Name</th>
          <th scope="col">Age</th>
          <th scope="col">Gender</th>
          <th scope="col">Jumlah Buku</th>
        </tr>
      </thead>
      <tbody>
        <% authors.map((el, index) => {%>
        <tr>
          <td><%= index +1 %></td>
          <td><%= el.name %></td>
          <td><%= el.age %></td>
          <td><%= el.gender %></td>
          <td>0</td>
        </tr>
        <% }) %>
      </tbody>
    </table>
    <!-- End Table -->
  </div>
</div>
<%- include("partials/footer") %>
